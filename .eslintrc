{
    "extends": "airbnb",
    "env": {
        "browser": true,
    },
    "installedESLint": true,
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "max-len": 0,
        "generator-star-spacing": 0,
        "no-param-reassign": 0,
        "no-bitwise": 0
    }
}
